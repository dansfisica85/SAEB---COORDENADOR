{
	"cleanUrls": true,
	"trailingSlash": false,
	"rewrites": [
		{ "source": "/", "destination": "/web/index.html" },
		{ "source": "/index.html", "destination": "/web/index.html" },
		{ "source": "/chat", "destination": "/web/chat.html" },
		{ "source": "/chat.html", "destination": "/web/chat.html" }
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{ "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
			]
		}
	]
}
